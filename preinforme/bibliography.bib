issn = {0278-3649},
journal = {The International Journal of Robotics Research},
keywords = {grounded reasoning methods,knowledge bases for robots,knowledge representation},
month = {apr},
number = {5},
pages = {566--590},
publisher = {SAGE PublicationsSage UK: London, England},
title = {{KnowRob: A knowledge processing infrastructure for cognition-enabled robots}},
url = {http://journals.sagepub.com/doi/10.1177/0278364913481635},
volume = {32},
year = {2013}
}
